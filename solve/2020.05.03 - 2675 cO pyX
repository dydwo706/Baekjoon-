#include<stdio.h>
#include<string.h>
int main(){
    int t,a;
    char b[21];
    
    scanf("%d", &t);
    
    for(int i=0;i<t;i++){
        scanf("%d", &a);
        getchar();
        scanf("%s", b);
        
        for(int j=0;j<strlen(b);j++){
            for(int k=0;k<a;k++)
                printf("%c", b[j]);
        }
        printf("\n");
    }
    
    return 0;
}
