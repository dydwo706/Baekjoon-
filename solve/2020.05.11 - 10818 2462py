N = int(input())

cho = list(map(int, input().split()))


print(min(cho), max(cho))

++++++++++++++++++++++++++
N = int(input())

cho = list(map(int, input().split()))

nmax = cho[0]
nmin = cho[0]

for i in range(N):
    if nmax < cho[i]:
        nmax = cho[i]
    if nmin > cho[i]:
        nmin = cho[i]

print(nmin, nmax)

-----------------------------
a = int(input())
num = 1
for i in range(8):
    b = int(input())
    if a < b:
        a = b
        num = i+2
print(a)

print(num)
