#include <stdio.h>

int main(){
    int k,n,i,T,j;
    int arr[15][15]={0};
    int sum=0;
    for(i=0;i<15;i++){
        arr[0][i]==i;
    }

    for(i=0;i<15;i++){
        arr[i][1]=1;
    }
    
    scanf("%d", &T);
    for(i=0;i<T;i++){
        scanf("%d", &k);
        scanf("%d", &n);

        for(i=1;i<=k;i++){
            for(j=1;j<=n;j++){
                arr[i][j]=arr[i-1][j]+arr[i][j-1];
            }
        }
    
        printf("%d\n", arr[k][n]);
    }

    
    return 0;
}


