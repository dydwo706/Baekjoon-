int cos(int q,int w){
	int a=1,b=0,f=1;
	if((q-w)<w) w=(q-w);
	
	while(1){
		if(b==w) break;
		a*=(q-b);
		b++;
	}
	while(1){
		if(w==0) break;
		f*=w;
		w--;
	}
	
	return a/w;
}
